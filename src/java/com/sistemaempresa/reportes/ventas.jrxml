		<queryString language="SQL">
			<![CDATA[SELECT vd.idVenta_detalle AS id, ve.fechaFactura, ve.noFactura, CONCAT(cl.nombres, ' ', cl.apellidos) AS nombre_cliente, cl.nit, cl.correo_electronico, pr.producto AS nombre_producto, vd.cantidad, vd.precio_unitario, (vd.cantidad * vd.precio_unitario) AS subtotal
FROM ventas_detalle AS vd
JOIN ventas AS ve ON vd.idVenta = ve.idVenta
JOIN productos AS pr ON vd.idProducto = pr.idProducto
JOIN clientes AS cl ON ve.idCliente = cl.idCliente
ORDER BY ve.fechaFactura;]]>
