		<queryString language="SQL">
			<![CDATA[SELECT cd.idcompra_detalle AS id, co.fecha_orden, co.no_orden_compra, pv.proveedor, pr.producto AS nombre_producto, cd.cantidad, cd.precio_costo_unitario, co.fechaingreso
